{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "build:sb": {
      "dependsOn": ["test:generate-output"],
      "outputs": ["storybook-static/**"]
    },
    "test": {
      "outputs": ["__report__/**"]
    },
    "test:generate-output": {
      "outputs": [".jest-test-results.json"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "storybook": {
      "cache": false
    },
    "start": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    }
  },
  "globalDependencies": [".env"],
  "globalEnv": ["VERCEL_URL", "RENDER_INTERNAL_HOSTNAME", "PORT"]
}
