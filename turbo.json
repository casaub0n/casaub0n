{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "outputs": [
        "__report__/**"
      ]
    },
    "dev": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "start:tsbox": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "start:parse-html": {
      "dependsOn": ["^build"],
      "env": ["TABLE_HEAD"],
      "outputs": ["dist/**"]
    },
    "start:sp": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev:me": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    }
  },
  "globalDependencies": [".env"],
  "globalEnv": ["VERCEL_URL", "RENDER_INTERNAL_HOSTNAME", "PORT"]
}
