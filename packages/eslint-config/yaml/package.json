{
  "name": "@casaub0n/eslint-config-yaml",
  "version": "0.0.0",
  "main": "./dist/index.mjs",
  "license": "MIT",
  "description": "yaml config for casaub0n",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "types": "./dist/index.d.mts",
    "default": "./dist/index.mjs"
  },
  "sideEffects": false,
  "files": [
    "./dist/index.d.mts",
    "./dist/index.mjs"
  ],
  "engines": {
    "node": ">=22"
  },
  "author": {
    "name": "casaub0n",
    "email": "netzach.yoshida@gmail.com",
    "url": "https://github.com/casaub0n"
  },
  "keywords": [
    "ESLint",
    "TypeScript",
    "Node.js",
    "JavaScript"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch src",
    "type:check": "tsc --noEmit",
    "lint": "eslint src/**"
  },
  "peerDependencies": {
    "eslint-plugin-yml": "catalog:"
  },
  "devDependencies": {
    "@casaub0n/eslint-base": "workspace:*",
    "@types/node": "catalog:",
    "@typescript-eslint/utils": "catalog:",
    "rolldown": "catalog:",
    "tsdown": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
