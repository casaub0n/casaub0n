{
  "name": "@casaub0n/eslint-config-utils",
  "version": "0.0.0",
  "main": "./dist/index.mjs",
  "license": "MIT",
  "description": "base eslint config for casaub0n",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "types": "./dist/*.d.mts",
    "default": "./dist/*.mjs"
  },
  "sideEffects": false,
  "files": [
    "./dist/ignore-config.d.mts",
    "./dist/ignore-config.mjs",
    "./dist/eslint-core-rules.d.mts",
    "./dist/eslint-core-rules.mjs",
    "./dist/typescript-rules.d.mts",
    "./dist/typescript-rules.mjs"
  ],
  "engines": {
    "node": ">=22"
  },
  "author": {
    "name": "casaub0n",
    "email": "netzach.yoshida@gmail.com",
    "url": "https://github.com/casaub0n"
  },
  "keywords": [
    "ESLint",
    "TypeScript",
    "Node.js",
    "JavaScript"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch src",
    "type:check": "tsc --noEmit",
    "lint": "eslint src/**"
  },
  "devDependencies": {
    "@casaub0n/eslint-base": "workspace:*",
    "@types/node": "catalog:",
    "@typescript-eslint/utils": "catalog:",
    "rolldown": "catalog:",
    "tsup": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
