{
  "name": "@casaub0n/eslint-config-utils",
  "version": "0.0.0",
  "license": "MIT",
  "description": "base eslint config for casaub0n",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "homepage": "https://github.com/casaub0n/casaub0n/tree/main/packages/eslint-config/utils#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./ignore-config": {
      "types": "./dist/ignore-config.d.mts",
      "default": "./dist/ignore-config.mjs"
    },
    "./eslint-core-rules": {
      "types": "./dist/eslint-core-rules.d.mts",
      "default": "./dist/eslint-core-rules.mjs"
    },
    "./typescript-rules": {
      "types": "./dist/typescript-rules.d.mts",
      "default": "./dist/typescript-rules.mjs"
    }
  },
  "module": "./dist/*.mjs",
  "types": "./dist/*.d.mts",
  "typesVersions": {
    "*": {
      "ignore-config": [
        "./src/ignore-config.ts"
      ],
      "eslint-core-rules": [
        "./src/eslint-core-rules.ts"
      ],
      "typescript-rules": [
        "./src/typescript-rules.ts"
      ]
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22"
  },
  "author": {
    "name": "casaub0n",
    "email": "netzach.yoshida@gmail.com",
    "url": "https://github.com/casaub0n"
  },
  "keywords": [
    "ESLint",
    "TypeScript",
    "Node.js",
    "JavaScript"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch src",
    "type:check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@typescript-eslint/utils": "catalog:",
    "eslint-typegen": "catalog:",
    "tsdown": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
