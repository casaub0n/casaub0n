{
  "name": "@casaub0n/eslint-config-utils",
  "version": "0.0.0",
  "main": "./dist/index.mjs",
  "license": "MIT",
  "description": "base eslint config for casaub0n",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "types": "./dist/index.d.mts",
    "default": "./dist/index.mjs"
  },
  "sideEffects": false,
  "files": [
    "./dist/ignore-config.d.mts",
    "./dist/ignore-config.mjs"
  ],
  "engines": {
    "node": ">=22"
  },
  "author": {
    "name": "casaub0n",
    "email": "netzach.yoshida@gmail.com",
    "url": "https://github.com/casaub0n"
  },
  "keywords": [
    "ESLint",
    "TypeScript",
    "Node.js",
    "JavaScript"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch src",
    "type:check": "tsc --noEmit",
    "lint": "pnpm dlx eslint src/**"
  },
  "devDependencies": {
    "@casaub0n/eslint-base": "workspace:*",
    "@types/node": "22.15.18",
    "@typescript-eslint/utils": "8.32.1",
    "rolldown": "1.0.0-beta.8",
    "tsup": "8.4.0",
    "tsx": "4.19.4",
    "typescript": "5.8.3"
  }
}
