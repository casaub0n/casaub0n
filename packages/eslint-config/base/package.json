{
  "name": "@casaub0n/eslint-base",
  "version": "0.0.0",
  "main": "./dist/index.mjs",
  "license": "MIT",
  "description": "base eslint config for casaub0n",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "types": "./dist/index.d.mts",
    "default": "./dist/index.mjs"
  },
  "sideEffects": false,
  "files": [
    "./dist/index.d.mts",
    "./dist/index.mjs"
  ],
  "engines": {
    "node": ">=22"
  },
  "author": {
    "name": "casaub0n",
    "email": "netzach.yoshida@gmail.com",
    "url": "https://github.com/casaub0n"
  },
  "keywords": [
    "ESLint",
    "TypeScript",
    "Node.js",
    "JavaScript"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch src",
    "type:check": "tsc --noEmit",
    "lint": "pnpm dlx eslint src/**"
  },
  "peerDependencies": {
    "@cspell/eslint-plugin": "catalog:",
    "@eslint/compat": "catalog:",
    "@eslint/eslintrc": "catalog:",
    "@eslint/js": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-import-resolver-typescript": "catalog:",
    "eslint-plugin-import": "catalog:",
    "eslint-plugin-import-x": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "eslint-plugin-turbo": "catalog:",
    "eslint-plugin-unicorn": "catalog:",
    "eslint-plugin-unused-imports": "catalog:",
    "eslint-plugin-yml": "catalog:",
    "globals": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "devDependencies": {
    "@types/eslint-config-prettier": "catalog:",
    "@types/node": "catalog:",
    "@typescript-eslint/utils": "catalog:",
    "tsdown": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
