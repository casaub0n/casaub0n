{
  "name": "@casaub0n/eslint-base",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "homepage": "https://github.com/casaub0n/casaub0n/tree/main/packages/eslint-config/base#readme",
  "description": "base eslint config for casaub0n",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22"
  },
  "author": {
    "name": "casaub0n",
    "email": "netzach.yoshida@gmail.com",
    "url": "https://github.com/casaub0n"
  },
  "keywords": [
    "ESLint",
    "TypeScript",
    "Node.js",
    "JavaScript"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch src",
    "type:check": "tsc --noEmit",
    "lint": "pnpm dlx eslint src/**",
    "dev": "eslint-config-inspector"
  },
  "peerDependencies": {
    "@cspell/eslint-plugin": "catalog:",
    "@eslint/compat": "catalog:",
    "@eslint/eslintrc": "catalog:",
