    "typescript-eslint": "8.32.1"
  },
  "devDependencies": {
    "@casaub0n/eslint-base": "workspace:*",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/node": "22.15.18",
    "@typescript-eslint/utils": "8.32.1",
