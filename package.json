{
  "license": "MIT",
  "name": "casaub0n",
  "private": true,
  "description": "my experimental apps and readme profile",
  "scripts": {
    "rs:build": "cargo build",
    "build": "turbo run build",
    "all:build": "run-p rs:build build",
    "test": "dotenv -- turbo run test",
    "test:ci": "dotenv -- turbo run test:ci",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "storybook": "turbo run storybook",
    "version:set": "pnpm exec lerna version",
    "lint": "turbo run lint",
    "spellcheck:lint": "cspell lint \"**/*.md\"",
    "spellcheck:fix": "cspell --words-only --unique \"**/*.md\" | sort --ignore-case | uniq"
  },
  "devDependencies": {
    "@cspell/cspell-types": "9.0.2",
    "@lerna-lite/cli": "4.3.0",
    "@lerna-lite/publish": "4.3.0",
    "@lerna-lite/version": "4.3.0",
    "cspell": "9.0.2",
    "dotenv-cli": "8.0.0",
    "eslint": "9.28.0",
    "git-cz": "4.9.0",
    "npm-run-all2": "catalog:",
    "prettier": "3.5.3",
    "renovate": "40.43.1",
    "textlint": "14.7.2",
    "textlint-filter-rule-allowlist": "4.0.0",
    "textlint-filter-rule-comments": "1.2.2",
    "textlint-rule-no-todo": "2.0.1",
    "textlint-rule-preset-ja-technical-writing": "12.0.2",
    "turbo": "2.5.4"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.11.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "core-js-pure",
      "msw",
      "prisma",
      "protobufjs",
      "puppeteer",
      "re2",
      "sharp",
      "unrs-resolver"
    ]
  }
}
